{
    "name": "rotex1800/registration",
    "type": "project",
    "description": "The source code for our event registration.",
    "keywords": [
        "laravel",
        "rotex1800",
        "youth-exchange",
        "registration"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1.0",
        "christophrumpel/missing-livewire-assertions": "^0.7.0",
        "comcast/php-legal-licenses": "^1.2",
        "fakerphp/faker": "^1.9.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/fortify": "^1.13",
        "laravel/framework": "^9.19",
        "laravel/tinker": "^2.7",
        "livewire/livewire": "^2.10",
        "maatwebsite/excel": "^3.1",
        "psr/simple-cache": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "brianium/paratest": "^6.6",
        "laravel/dusk": "^7.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.2",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "pestphp/pest-plugin-parallel": "^1.2",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0",
        "staudenmeir/dusk-updater": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "phpstan": [
            "./vendor/bin/phpstan analyse"
        ]
    },
    "scripts-descriptions": {
        "phpstan": "Run PHPStan static analysis against your application."
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
